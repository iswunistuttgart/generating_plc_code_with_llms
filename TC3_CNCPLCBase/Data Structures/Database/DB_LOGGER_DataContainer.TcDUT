<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.11">
  <DUT Name="DB_LOGGER_DataContainer" Id="{9bcae129-09fe-4eba-84af-37cf10e5bacf}">
    <Declaration><![CDATA[(****************************************************************************************
Author: 	ISW
Date: 		2022-01-24
Version:	1.0
------------------------------------------------------------------------------------
Description:

This data structure contains the output data container for the logger .

****************************************************************************************)
TYPE DB_LOGGER_DataContainer :
STRUCT

	// The scanner positions of the x axis in mm.
	{attribute 'Fieldname' := 'x position'}
	positions_x : LREAL := -1;
	
	// The scanner positions of the y axis in mm.
	{attribute 'Fieldname' := 'y position'}
	positions_y : LREAL := -1;
	
	// The scanner positions of the z axis in mm.
	{attribute 'Fieldname' := 'z position'}
	positions_z : LREAL := -1;
	
	// The scanner positions of the a axis in degree.
	{attribute 'Fieldname' := 'z position'}
	positions_b : LREAL := -1;
	
	// The scanner positions of the c axis in degree.
	{attribute 'Fieldname' := 'z position'}
	positions_c : LREAL := -1;
	
	// The number of the corrently used Databuffer (for debugging)
	{attribute 'Fieldname' := 'Buffer'}
	//buffer : LREAL := -1;

	// Current System Time 
	{attribute 'Fieldname' := 'Time'}
	//sysTime : ULINT;
		
	// counter for debugging if all Data is written or if some data is lost
	{attribute 'Fieldname' := 'counter'} 		
	// counter : LWORD;
	
END_STRUCT
END_TYPE
]]></Declaration>
  </DUT>
</TcPlcObject>