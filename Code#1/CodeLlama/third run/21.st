FUNCTION BURNER : BOOL;
VAR
    PRE_HEAT : BOOL;
    OIL_TEMP : BOOL;
    STAGE2 : BOOL;
END_VAR
IF IN = FALSE THEN
    IF OVER_TEMP = TRUE THEN
        FAIL := TRUE;
        STATUS := 9;
    ELSE
        PRE_HEAT := TRUE;
        WHILE PRE_HEAT DO
            WAIT FOR CHANGE OF OIL_TEMP;
            IF OIL_TEMP = FALSE THEN
                FAIL := TRUE;
                STATUS := 1;
                EXIT;
            END_IF;
        END_WHILE;
        STAGE2 := TRUE;
    END_IF;
ELSIF IN = TRUE AND PRE_HEAT = FALSE THEN
    IF OVER_TEMP = TRUE THEN
        FAIL := TRUE;
        STATUS := 9;
    ELSE
        WHILE NOT STAGE2 DO
            WAIT FOR CHANGE OF STAGE2;
            IF STAGE2 = FALSE THEN
                FAIL := TRUE;
                STATUS := 10;
                EXIT;
            END_IF;
        END_WHILE;
    END_IF;
END_IF;
STATUS := 110;
BURNER := TRUE;
