IF (HUMIDITY < 40 OR HUMIDITY > 75) THEN
    DISPLAY "Humidity out of range!"
ELSEIF (TEMPERATURE < 18 OR TEMPERATURE > 25) THEN
    IF (TEMPERATURE < 18) THEN
        DISPLAY "Cold"
    ELSEIF (TEMPERATURE > 25) THEN
        DISPLAY "Hot"
    END_IF
ELSE
    IF (HUMIDITY BETWEEN 40 AND 75 AND TEMPERATURE BETWEEN 18 AND 25) THEN
        DISPLAY "Opt"
    ELSE
        DISPLAY "Temp. OK"
    END_IF
END_IF