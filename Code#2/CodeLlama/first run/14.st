FUNCTION FC1001 :BOOL
VAR_INPUT
	MEW: REAL;
	SP: REAL;
	HYS: REAL;
END_VAR
VAR_IN_OUT
 	OUT:BOOL;
END_VAR
VAR
 	P_HYS: REAL;
END_VAR
BEGIN
  IF (MEW > SP) THEN
    P_HYS := HYS / 100 * SP;
    IF (MEW - P_HYS > 0) THEN
      OUT := TRUE;
    ELSE
      OUT := FALSE;
    END_IF;
  ELSE
    P_HYS := HYS / 100 * MEW;
    IF (P_HYS - SP > 0) THEN
      OUT := TRUE;
    ELSE
      OUT := FALSE;
    END_IF;
  END_IF;
END_FUNCTION
